{
  "description": [
      "Extensions of HTML attribute parsing for common browser features."
      ],

  "types": [
      { "key": "*::NAME", "description": "other name",
        "type": "GLOBAL_NAME", "optional": true },

      { "key": "*::ONBLUR", "type": "SCRIPT", "optional": true,
        "comment": [
          "html4 defines this attribute only for specific elements.",
          "But events bubble, so any element can get this event.",
          "html5 explicitly allows this on any element." ] },
      { "key": "*::ONCHANGE", "type": "SCRIPT", "optional": true },
      { "key": "*::ONFOCUS", "type": "SCRIPT", "optional": true },
      { "key": "*::ONLOAD", "type": "SCRIPT", "optional": true },
      { "key": "*::ONRESET", "type": "SCRIPT", "optional": true },
      { "key": "*::ONSELECT", "type": "SCRIPT", "optional": true },
      { "key": "*::ONSUBMIT", "type": "SCRIPT", "optional": true },
      { "key": "*::ONUNLOAD", "type": "SCRIPT", "optional": true },

      { "key": "CANVAS::HEIGHT", "description": "canvas height",
        "pattern": "[0-9]+", "optional": true },

      { "key": "CANVAS::WIDTH", "description": "canvas width",
        "pattern": "[0-9]+", "optional": true },

      { "key": "FORM::AUTOCOMPLETE",
        "description": "http://www.whatwg.org/specs/web-apps/current-work/multipage/forms.html#attr-form-autocomplete",
        "optional": true, "default": "on", "values": "off",
        "comment": "The on value is explicitly excluded from the values list."
      },

      { "key": "INPUT::AUTOCOMPLETE",
        "description": "http://www.whatwg.org/specs/web-apps/current-work/multipage/common-input-element-attributes.html#attr-input-autocomplete",
        "optional": true, "default": "on", "values": "off",
        "comment": "The on value is explicitly excluded from the values list."
      }

      ],

  "allowed": [
      "*::ONBLUR",
      "*::ONCHANGE",
      "*::ONFOCUS",
      "*::ONBLUR",
      "*::ONLOAD",
      "*::ONRESET",
      "*::ONSELECT",
      "*::ONSUBMIT",
      "*::ONUNLOAD",
      "CANVAS::HEIGHT",
      "CANVAS::WIDTH",
      "FORM::AUTOCOMPLETE",
      "INPUT::AUTOCOMPLETE"
      ]
}
